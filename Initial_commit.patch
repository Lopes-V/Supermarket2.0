+        service.save(p.getName(),p.getValue(),p.getCategoria(),p.getQuantity());
+    }
+
+    @DeleteMapping("/{id}")
+    public void deleteProduct(@PathVariable UUID id){
+        service.delete(id);
+    }
+}
